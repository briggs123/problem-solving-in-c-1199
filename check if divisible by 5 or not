#include<stdio.h>
int main()
{
    int a;
    printf("eneter the number :");
    scanf("%d",&a);
    if(a%5==0 && a%11==0)
    {
        printf("the number is divisible by both 5 and 11");
    }
    else if(a%5==0)
    {
        printf("the value is only divisible by 5");
    }
    else
    {
        printf("the value is only divisible by 11");
    }
    return 0;
}
