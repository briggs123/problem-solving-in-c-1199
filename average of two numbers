#include<stdio.h>
int main()
{
    int a,b,c;
    float avg;
    printf("the value of a");
    scanf("%d",&a);
    printf("the value of b");
    scanf("%d",&b);
    printf("the value of c");
    scanf("%d",&c);
    avg=(a+b+c)/3;
    printf("the avg of the numbers%f",avg);
    return 0;
    
    
}
