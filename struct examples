#include<stdio.h>
struct info
{
    int age;
    float marks;
    char name[20];
}student[10];
int main()
{
    int i;
    for(i=0;i<10;i++)
    {
    printf("enter the information of the %d student ",i+1);
    scanf("%d%f%s",&student[i].age,&student[i].marks,student[i].name);
}
for(i=0;i<10;i++)
{
    printf("\n\ninformation of student %d",i+1);
    printf("\nname== %s\nage== %d\nmarks== %f",student[i].name,student[i].age,student[i].marks);
}
return 0;
}
    
