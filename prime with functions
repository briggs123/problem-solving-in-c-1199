#include<stdio.h>
int prime(int a)
{
    int i,c=0;
    for(i=1;i<=a;i++)
    {
         
        if(a%i==0)
        {
           c++;
           
        }
    
    }
  
    return c;
}
int main()
{
    int i,check1,check2;
    printf("enter the value");
    scanf("%d",&i);
check1=prime(i);
if(check1==2)
{
    printf("it is a prime");
}
else
{
    printf("not a prime");
}
    return 0;
}


