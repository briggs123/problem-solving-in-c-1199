#include<stdio.h>
int main()
{
    int a,b,c;
    printf("enter the value of a,b,c");
    scanf("%d %d %d",&a,&b,&c);
    if(a<b && b<c)
    {
        printf("a is minimum");
    }
    else if(b<c && b<a)
    {
        printf("b is minimum");
    }
    else
    {
        printf("c is minimum");
    }
    return 0;
}
